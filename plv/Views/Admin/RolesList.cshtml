@model plv.ViewModels.ManageRoleViewModel

@{
    ViewData["Title"] = "ManageRoles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-8">
        <div class="plog">
            <h1>ManageRoles</h1>

            @foreach (var role in Model.Role)
            {
                <p><a href="/Admin/ManageRole/@role.Id">@role.Name</a></p>
            }
        </div>
    </div>
</div>